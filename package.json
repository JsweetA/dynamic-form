{
  "name": "hy-form",
  "version": "1.0.1",
  "description": "",
  "main": "dist/Hy-form.mjs",
  "module": "dist/Hy-form.umd.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint ./ --ext **/*.{js,jsx,tsx,ts,less,json,vue} --max-warnings=0",
    "lint-fix": "eslint --fix ./ --ext **/*.{js,jsx,tsx,ts,less,json,vue} --max-warnings=0",
    "format": "prettier --write **/*.{js,jsx,tsx,ts,less,json,vue}",
    "format-watch": "onchange -d 1000 **/*.{js,jsx,tsx,ts,less,json,vue} -- prettier --config .prettierrc --write {{changed}}",
    "commit": "git add -A && cz && git push",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve:demo-vue3": "cd ./demo/vue3 && pnpm serve",
    "serve:arco-dev": "cd ./packages/arco && vite",
    "mp": "cd ./examples/uniapp-demo && pnpm mp",
    "build:arco": "cd ./packages/arco && pnpm build"
  },
  "bugs": {
    "url": "https://github.com/JsweetA/hy-form/issues"
  },
  "homepage": "https://github.com/JsweetA/hy-form#readme",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.0.3",
    "terser": "^5.20.0"
  }
}
